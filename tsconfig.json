{
  "extends": "./node_modules/gts/tsconfig-google.json",
  "compilerOptions": {
    "rootDir": ".",
    "outDir": "build",
    "module": "ES6",
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "target": "ESNext",
    "noImplicitAny": true,
    "sourceMap": false,
    "strict": false,
  },
  "include": [
    "src/**/*.ts",
    "test/**/*.ts"
  ]
}
